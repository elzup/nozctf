{
  "name": "scoreform",
  "version": "1.0.0",
  "description": "scoreform web app",
  "engines": {
    "node": "10"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next src/app",
    "lint": "eslint --ext .ts,.tsx src",
    "preserve": "npm run build-public && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "NODE_ENV=production firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-app": "next build \"src/app\"",
    "build-public": "cpx \"src/app/public/**/*.*\" \"dist/public\" -C",
    "build-functions": "tsc --project src/functions",
    "typecheck-app": "tsc --project src/app",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "firebase": "^7.6.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.0",
    "firestore-simple": "^6.0.1",
    "formik": "^2.1.4",
    "next": "^9.3.4",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.1.1",
    "styled-components": "4.4.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "4.1.8",
    "@types/yup": "^0.26.34",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-nzap": "^1.4.4",
    "firebase-tools": "^8.0.2",
    "rimraf": "3.0.2",
    "typescript": "^3.2.0"
  }
}
